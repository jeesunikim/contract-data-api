model transactions {
  id               String  @id
  transaction_hash String?
  ledger_sequence  String?
  account          String?
  account_sequence String?
  tx_envelope      String?
  tx_result        String?
  tx_meta          String?
  tx_fee_meta      String?

  @@index([transaction_hash], map: "idx_transaction_hash")
}
